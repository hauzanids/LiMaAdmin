<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".TertinggiUser">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="155dp"
        android:layout_marginBottom="61dp"
        android:background="@android:color/white"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="670dp"
        android:background="@drawable/rectangle6"/>

    <Button
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="680dp"
        android:layout_marginLeft="90dp"
        android:background="@drawable/user_black"/>

    <View
        android:id="@+id/button_group"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="680dp"
        android:layout_marginLeft="285dp"
        android:background="@drawable/group_white"/>

    <View
        android:id="@+id/rectangle2"
        android:layout_width="match_parent"
        android:layout_height="255.92dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-100dp"
        android:background="@drawable/rectangle2"
        />

    <View
        android:id="@+id/rectangle"
        android:layout_width="match_parent"
        android:layout_height="251.14dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-100dp"
        android:background="@drawable/rectangle" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_alignBottom="@+id/rectangle2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="85dp"
        android:gravity="center"
        android:text="List Kehadiran Mahasiswa"
        android:textSize="20sp" />

</RelativeLayout>