<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".TertinggiGroup">

    <View
        android:id="@+id/rectangle2"
        android:layout_width="match_parent"
        android:layout_height="255.92dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-100dp"
        android:background="@drawable/rectangle2"
        />

    <View
        android:id="@+id/rectangle"
        android:layout_width="match_parent"
        android:layout_height="251.14dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-100dp"
        android:background="@drawable/rectangle" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_alignBottom="@+id/rectangle2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:layout_marginBottom="85dp"
        android:gravity="center"
        android:text="Nilai Tertinggi Kelompok"
        android:textSize="20sp" />

    <View
        android:id="@+id/rectangle3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="180dp"
        android:layout_marginBottom="400dp"
        android:layout_marginRight="50dp"
        android:layout_marginLeft="50dp"
        android:background="@drawable/max" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignStart="@+id/rectangle3"
        android:layout_alignLeft="@+id/rectangle3"
        android:layout_alignTop="@+id/rectangle3"
        android:layout_alignEnd="@+id/rectangle3"
        android:layout_alignRight="@+id/rectangle3"
        android:layout_alignBottom="@+id/rectangle3"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="101dp"
        android:gravity="center"
        android:text="Kelompok Terbaik"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignStart="@+id/rectangle3"
        android:layout_alignLeft="@+id/rectangle3"
        android:layout_alignTop="@+id/rectangle3"
        android:layout_alignEnd="@+id/rectangle3"
        android:layout_alignRight="@+id/rectangle3"
        android:layout_alignBottom="@+id/rectangle3"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="50dp"
        android:gravity="center"
        android:text="Kelompok 2"
        android:textSize="30sp"
        android:textColor="#000000"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignStart="@+id/rectangle3"
        android:layout_alignLeft="@+id/rectangle3"
        android:layout_alignTop="@+id/rectangle3"
        android:layout_alignEnd="@+id/rectangle3"
        android:layout_alignRight="@+id/rectangle3"
        android:layout_alignBottom="@+id/rectangle3"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="100dp"
        android:gravity="center"
        android:text="Poin 100"
        android:textSize="25sp"
        android:textColor="#000000"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="350dp"
        android:layout_marginBottom="61dp"
        android:background="@android:color/white"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="670dp"
        android:background="@drawable/rectangle6"/>

    <Button
        android:id="@+id/button_user"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="680dp"
        android:layout_marginLeft="90dp"
        android:background="@drawable/user_white"/>

    <View
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="680dp"
        android:layout_marginLeft="285dp"
        android:background="@drawable/group_black"/>

</RelativeLayout>