<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="25dp"
    android:layout_marginRight="25dp"
    android:layout_marginTop="12.5dp"
    android:layout_marginBottom="12.5dp"
    app:cardCornerRadius="25dp"
    android:id="@+id/tertinggi_user_item">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="20dp"/>

    <View
        android:id="@+id/rectangle3"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/rectangle4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="20dp"
        android:text="Kel"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textViewKel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="40dp"
        android:text="2"
        android:textColor="#000000" />

    <View
        android:layout_width="1dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/black" />

    <TextView
        android:id="@+id/textViewNama"
        android:layout_width="230dp"
        android:layout_height="39dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:text="Hauzani Dhianulwansyah Setiawan"
        android:lineSpacingExtra="-6dp"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewSprint"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="50dp"
        android:text="Sprint 10"
        android:gravity="center_vertical"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textViewJamMulai"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginStart="170dp"
        android:layout_marginTop="50dp"
        android:text="07:00"
        android:gravity="right"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textViewJamAkhir"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginStart="232dp"
        android:layout_marginTop="50dp"
        android:text="17:00"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="14sp" />

    <View
        android:layout_width="1dp"
        android:layout_height="50dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/black" />

    <TextView
        android:id="@+id/textViewNilai"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="300dp"
        android:layout_marginTop="20dp"
        android:text="100"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="17sp" />

</androidx.cardview.widget.CardView>